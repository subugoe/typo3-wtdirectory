<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<mainconfig>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:wt_directory/locallang_db.xml:pi_flexform.sheet_mainconfig</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<addresspool>
						<TCEforms>
							<label>LLL:EXT:wt_directory/locallang_db.xml:pi_flexform.address</label>
							<config>
								<type>select</type>
								<items type="array">

								</items>
								<maxitems>100</maxitems>
								<size>5</size>
								<autoSizeMax>10</autoSizeMax>
								<minitems>0</minitems>
								<selectedListStyle>width:150px</selectedListStyle>
								<itemListStyle>width:300px</itemListStyle>
								<itemsProcFunc>user_be_address->main</itemsProcFunc>
								<allowNonIdValues>1</allowNonIdValues>
							</config>
						</TCEforms>
					</addresspool>
					<cat_join>
						<TCEforms>
							<label>LLL:EXT:wt_directory/locallang_db.xml:pi_flexform.cat_join</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:wt_directory/locallang_db.xml:pi_flexform.cat_join.0</numIndex>
										<numIndex index="1">0</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:wt_directory/locallang_db.xml:pi_flexform.cat_join.1</numIndex>
										<numIndex index="1">1</numIndex>
									</numIndex>
								</items>
								<size>1</size>
							</config>
						</TCEforms>
					</cat_join>
					<category>
						<TCEforms>
							<label>LLL:EXT:wt_directory/locallang_db.xml:pi_flexform.category</label>
							<config>
								<type>select</type>
								<form_type>user</form_type>
								<userFunc>tx_ttaddress_treeview->displayGroupTree</userFunc>
								<treeView>1</treeView>
								<foreign_table>tt_address_group</foreign_table>
								<foreign_table_where>ORDER BY tt_address_group.sorting</foreign_table_where>
								<size>5</size>
								<autoSizeMax>10</autoSizeMax>
								<minitems>0</minitems>
								<maxitems>100</maxitems>
								<items type="array">

								</items>
								<allowNonIdValues>1</allowNonIdValues>
							</config>
						</TCEforms>
					</category>
					<shownone>
						<TCEforms>
							<label>LLL:EXT:wt_directory/locallang_db.xml:pi_flexform.shownone</label>
							<config>
								<type>check</type>
								<items type="array">
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:wt_directory/locallang_db.xml:pi_flexform.shownone.0</numIndex>
										<numIndex index="1">1</numIndex>
									</numIndex>
								</items>
								<default>0</default>
							</config>
						</TCEforms>
					</shownone>
				</el>
			</ROOT>
		</mainconfig>
		<list>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:wt_directory/locallang_db.xml:pi_flexform.sheet_list</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<target>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:wt_directory/locallang_db.xml:pi_flexform.target_detail</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</target>
					<field>
						<TCEforms>
							<label>LLL:EXT:wt_directory/locallang_db.xml:pi_flexform.field</label>
							<config>
								<type>select</type>
								<items type="array">

								</items>
								<maxitems>100</maxitems>
								<size>10</size>
								<autoSizeMax>10</autoSizeMax>
								<minitems>0</minitems>
								<selectedListStyle>width:200px</selectedListStyle>
								<itemListStyle>width:200px</itemListStyle>
								<itemsProcFunc>user_be_fields->main</itemsProcFunc>
								<allowNonIdValues>1</allowNonIdValues>
							</config>
						</TCEforms>
					</field>
					<abc>
						<TCEforms>
							<label>LLL:EXT:wt_directory/locallang_db.xml:pi_flexform.abc</label>
							<config>
								<type>select</type>
								<items type="array">

								</items>
								<maxitems>1</maxitems>
								<size>1</size>
								<minitems>1</minitems>
								<itemsProcFunc>user_be_abcfields->main</itemsProcFunc>
								<allowNonIdValues>1</allowNonIdValues>
								<default>name</default>
							</config>
						</TCEforms>
					</abc>
					<search>
						<TCEforms>
							<label>LLL:EXT:wt_directory/locallang_db.xml:pi_flexform.search</label>
							<config>
								<type>select</type>
								<items type="array">

								</items>
								<maxitems>100</maxitems>
								<size>3</size>
								<autoSizeMax>10</autoSizeMax>
								<minitems>0</minitems>
								<selectedListStyle>width:200px</selectedListStyle>
								<itemListStyle>width:200px</itemListStyle>
								<itemsProcFunc>user_be_fields->main</itemsProcFunc>
								<itemsProcFuncArg>searchAll</itemsProcFuncArg>
								<allowNonIdValues>1</allowNonIdValues>
							</config>
						</TCEforms>
					</search>
					<enablecatchoose>
						<TCEforms>
							<label>LLL:EXT:wt_directory/locallang_db.xml:pi_flexform.enablecatchoose</label>
							<config>
								<type>check</type>
								<items type="array">
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:wt_directory/locallang_db.xml:pi_flexform.enable</numIndex>
										<numIndex index="1">1</numIndex>
									</numIndex>
								</items>
								<default>1</default>
							</config>
						</TCEforms>
					</enablecatchoose>
					<radialsearch>
						<TCEforms>
							<label>LLL:EXT:wt_directory/locallang_db.xml:pi_flexform.radialsearch</label>
							<config>
								<type>check</type>
								<items type="array">
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:wt_directory/locallang_db.xml:pi_flexform.enable</numIndex>
										<numIndex index="1">1</numIndex>
									</numIndex>
								</items>
								<default>0</default>
							</config>
						</TCEforms>
					</radialsearch>
				</el>
			</ROOT>
		</list>
		<detail>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:wt_directory/locallang_db.xml:pi_flexform.sheet_detail</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<target>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:wt_directory/locallang_db.xml:pi_flexform.target_list</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</target>
					<field>
						<TCEforms>
							<label>LLL:EXT:wt_directory/locallang_db.xml:pi_flexform.field</label>
							<config>
								<type>select</type>
								<items type="array">

								</items>
								<maxitems>100</maxitems>
								<size>10</size>
								<autoSizeMax>10</autoSizeMax>
								<minitems>0</minitems>
								<selectedListStyle>width:200px</selectedListStyle>
								<itemListStyle>width:200px</itemListStyle>
								<itemsProcFunc>user_be_fields->main</itemsProcFunc>
								<allowNonIdValues>1</allowNonIdValues>
							</config>
						</TCEforms>
					</field>
				</el>
			</ROOT>
		</detail>
		<googlemap>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:wt_directory/locallang_db.xml:pi_flexform.sheet_googlemap</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<error>
						<TCEforms>
							<label>LLL:EXT:wt_directory/locallang_db.xml:pi_flexform.status</label>
							<config>
								<type>user</type>
								<userFunc>user_be_googlemapmsg->googlemapmsg</userFunc>
							</config>
						</TCEforms>
					</error>
					<enable>
						<TCEforms>
							<label>LLL:EXT:wt_directory/locallang_db.xml:pi_flexform.googlemapenable</label>
							<config>
								<type>check</type>
								<items type="array">
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:wt_directory/locallang_db.xml:pi_flexform.enable</numIndex>
										<numIndex index="1">1</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</enable>
					<target>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:wt_directory/locallang_db.xml:pi_flexform.googlemap_page</label>
							<config>

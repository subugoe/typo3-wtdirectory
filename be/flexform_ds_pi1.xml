				<TCEforms>
					<sheetTitle>LLL:EXT:wt_directory/locallang_db.xml:pi_flexform.sheet_tt_news</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<contact>
						<TCEforms>
							<label>LLL:EXT:wt_directory/locallang_db.xml:pi_flexform.contact</label>
							<config>
								<type>check</type>
								<items type="array">
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:wt_directory/locallang_db.xml:pi_flexform.contactitem</numIndex>
										<numIndex index="1">1</numIndex>
									</numIndex>
